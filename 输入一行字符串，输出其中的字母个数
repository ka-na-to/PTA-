#include<stdio.h>    //  输入一行字符串，输出其中的字母个数       
int main(){
	char a[20];     //定义一个字符数组
	int i = 0,j = 0;   //后面要进行一个i++进行遍历，要有一个开始的值，所以要进行赋值
 scanf("%s",a);       //输入该行字符串，注意使用的是%s;然后a是数组，数组名就是地址
 for(;a[i]!='\0';i++){      
	 if((a[i]>='A'&&a[i]<= 'Z')||(a[i]>='a'&&a[i]<='z')){
		 j  = j+1;      
	 }
 }
 printf("%d\n",j);
return 0;
}
